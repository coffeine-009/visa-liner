{
  "manifest_version": 2,

  "name": "Visa-liner",
  "description": "Registration for getting visa.",
  "version": "1.0",

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://visapoint.eu/*"],
//      "css": ["mystyles.css"],
      "js": [
//        "jquery.js",
        "content.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "notifications",
    "storage",
    "https://visapoint.eu/",
    "https://rucaptcha.com/"
  ]
}
