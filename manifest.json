{
  "manifest_version": 2,

  "name": "Visa-liner",
  "description": "Registration for getting visa.",
  "version": "1.0",

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "activeTab", 
    "http://www.polandvisa-ukraine.com/"
  ]
}
